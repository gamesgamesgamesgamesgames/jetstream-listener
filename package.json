{
  "name": "listener",
  "private": true,
  "license": "BSD-3-Clause",
  "type": "module",
  "scripts": {
    "dev:build": "rollup --config --watch",
    "dev:start": "dotenvx run -- node --watch ./dist/index.js",
    "dev": "yarn run setup && concurrently 'yarn run dev:build' 'yarn run dev:start'",
    "build": "rollup --config",
    "build:lexicons": "lex build --lexicons ./lexicons --out ./src/lexicons",
    "setup": "yarn run build:lexicons && yarn run build",
    "start": "dotenvx run -- node ./dist/index.js",
    "lint": "eslint ./src/"
  },
  "dependencies": {
    "@atproto/lex": "^0.0.11",
    "@dotenvx/dotenvx": "^1.51.4",
    "@skyware/jetstream": "^0.2.5",
    "@supabase/supabase-js": "^2.89.0",
    "winston": "^3.19.0",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "@birbhouse/eslint-config": "^1.2.0",
    "@birbhouse/eslint-config-typescript": "^1.1.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/eslint": "^9.6.1",
    "@types/eslint-plugin-security": "^3.0.0",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/node": "^24.10.4",
    "@typescript-eslint/eslint-plugin": "8.38.0",
    "@typescript-eslint/parser": "8.38.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-editorconfig": "^4.0.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsdoc": "^51.4.1",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sort-class-members": "^1.21.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "rollup": "^4.55.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  },
  "packageManager": "yarn@4.12.0"
}
